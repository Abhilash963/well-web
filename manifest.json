{
    "manifest_version": 3,
    "name": "well web",
    "version": "1.0",
    "description": "Monitor internet habits and receive personalized health alerts.",
    "permissions": [
      "tabs", 
      "activeTab", 
      "storage", 
      "notifications", 
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    },
    "options_page": "options.html"
  }
  